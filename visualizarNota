class VisualizarNotasWidget extends StatefulWidget {
  const VisualizarNotasWidget({
    Key? key,
    required this.refHomePage,
  }) : super(key: key);

  final DocumentReference? refHomePage;

  @override
  _VisualizarNotasWidgetState createState() => _VisualizarNotasWidgetState();
}

class _VisualizarNotasWidgetState extends State<VisualizarNotasWidget> {
