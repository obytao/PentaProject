class NotaWidget extends StatefulWidget {
  const NotaWidget({Key? key}) : super(key: key);

  @override
  _NotaWidgetState createState() => _NotaWidgetState();
}

class _NotaWidgetState extends State<NotaWidget> {
  late NotaModel _model;

  final scaffoldKey = GlobalKey<ScaffoldState>();

  @override
  void initState() {
    super.initState();
    _model = createModel(context, () => NotaModel());

    _model.tituloController ??= TextEditingController();
    _model.dataController ??= TextEditingController();
    _model.horaentradaController ??= TextEditingController();
    _model.horasaidaController ??= TextEditingController();
    _model.noteController ??= TextEditingController();
    WidgetsBinding.instance.addPostFrameCallback((_) => setState(() {
          _model.dataController?.text = dateTimeFormat(
            'd/M/y',
            getCurrentTimestamp,
            locale: FFLocalizations.of(context).languageCode,
          );
          _model.horaentradaController?.text = dateTimeFormat(
            'Hm',
            getCurrentTimestamp,
            locale: FFLocalizations.of(context).languageCode,
          );
        }));
  }

  @override
  void dispose() {
    _model.dispose();

    super.dispose();
  }

